{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env",
    ".env.local",
    "**/.env.*local",
    "**/config-eslint/index.js"
  ],
  "globalEnv": ["RABBITMQ_QUEUE", "REDIS_HOST", "REDIS_PORT"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
